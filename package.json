{
  "name": "plang-interpreter",
  "version": "0.0.1",
  "description": "A Plang Language Interpreter",
  "scripts": {
    "build-browser": "tsc --module AMD --outFile \"./out/index.js\"",
    "build-cli": "npm run build && pkg .",
    "build": "tsc",
    "run": "node out/index.js",
    "run-build": "npm run build && npm run run"
  },
  "pkg": {
    "scripts": "out/**/*.js",
    "targets": [
      "node12-win-x64"
    ],
    "outputPath": "dist"
  },
  "bin": "./out/index.js",
  "keywords": [],
  "author": "Terry Qi <troppydash.developer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.37",
    "pkg": "^4.5.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "prompt-sync": "^4.2.0"
  }
}
