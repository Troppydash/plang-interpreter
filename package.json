{
  "name": "plang-interpreter",
  "version": "0.0.1",
  "description": "A Plang Language Interpreter",
  "repository": "https://github.com/Troppydash/plang-interpreter/",
  "scripts": {
    "build-browser": "node ./scripts/timestamp.js && tsc --module AMD --outFile \"./out/index.js\"",
    "build-cli": "node ./scripts/timestamp.js && npm run build && pkg .",
    "build": "tsc",
    "run": "node out/index.js",
    "run-build": "npm run clean && npm run build && npm run run",
    "watch-build": "tsc --watch",
    "clean": "tsc --build --clean"
  },
  "pkg": {
    "scripts": "out/**/*.js",
    "targets": [
      "node12-win-x64",
      "node12-linux-x64"
    ],
    "outputPath": "dist",
    "assets": [
      "node_modules/blessed/**/*",
      "node_modules/deasync/**/*"
    ]
  },
  "bin": "./out/index.js",
  "keywords": [],
  "author": "Terry Qi <troppydash.developer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/blessed": "^0.1.17",
    "@types/node": "^14.14.37",
    "pkg": "^4.5.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "deasync": "^0.1.21",
    "node-fetch": "^2.6.1",
    "prompt-sync": "^4.2.0",
    "prompt-sync-history": "^1.0.1"
  }
}
